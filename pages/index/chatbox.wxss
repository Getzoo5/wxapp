/* pages/index/chatbox.wxss */
.weui-cell {
  padding: 7px 10px;
}
.weui-cell::after,.weui-cell::before{
  left: 0
}
.weui-cells::before{
  border: none
}

/* search */
.page__bd{
  position: relative
}
.weui-search-bar{
  background: #fff;
  border: none
}
.weui-search-bar__form,.weui-search-bar__label{
  color: #A0A0A0;
  background: #F3F3F3
}
.weui-search-bar__form,.weui-search-bar__label,.weui-search-bar__box,.weui-search-bar__input{
  line-height:24px;
  height:26px;
  background: #F3F3F3;
  border: none;
  outline: none;
}
.weui-search-bar__box{
  color: #A0A0A0;
}
.weui-search-bar__input{
  font-size: 26rpx;
}
.weui-search-bar__text,weui-search-bar__label{
  font-size: 26rpx;
  line-height: 22rpx
}
.weui-search-bar__cancel-btn{
  font-size: 28rpx;
  line-height:26px
}
.searchbar-result{
  width: 100%;
    margin-top: 0;
    font-size: 14px;
    position: absolute;
    top:96rpx;
    left: 0;
    z-index: 2
}
.searchbar-result:before{
    display: none;
}